<?xml version="1.0" encoding="utf-8"?>
<!--
     AdmiralUI
     Generated by Fastlane Figma Plugin.
-->
<resources>

    <style name="AdmiralTextAppearance" parent="TextAppearance.AppCompat">
        <item name="android:includeFontPadding">false</item>
    </style>
    {% for style in styles %}
    <style name="AdmiralTextAppearance.{{ style.name_info.style_name | remove: "_" | remove: " "}}">
        <item name="android:textSize">{{ style.font.font_size | round }}sp</item>
        <item name="android:fontFamily">@font/roboto_{{ style.font.font_weight }}</item>
        <item name="android:letterSpacing">{{ style.font.letter_spacing | divided_by: 10 }}</item>
    </style>

    <style name="AdmiralTextAppearance.{{ style.name_info.style_name | remove: "_" | remove: " "}}.Fixed">
        <item name="android:textSize">{{ style.font.font_size | round }}dp</item>
    </style>
    {% endfor %}
    <style name="AdmiralTextAppearance.PinNum">
        <item name="android:textSize">36sp</item>
        <item name="android:fontFamily">@font/roboto_400</item>
    </style>

    <style name="AdmiralTextAppearance.PinNum.Fixed">
        <item name="android:textSize">36dp</item>
    </style>

    <style name="AdmiralTextAppearance.TimePickerChips">
        <item name="android:textSize">42sp</item>
        <item name="android:fontFamily">@font/roboto_400</item>
    </style>

    <style name="AdmiralTextAppearance.TimePickerClockFace">
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/roboto_500</item>
    </style>

    <style name="AdmiralTextAppearance.TimePickerOverline" parent="AdmiralTextAppearance.OVERLINE">
        <item name="android:textAllCaps">true</item>
    </style>

</resources>